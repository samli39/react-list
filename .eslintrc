{
  "parser": "babel-eslint",
  "extends":["airbnb","prettier/react"],
  "env": {
    "es6": true,
    "browser":true,
    "node":true
  },
  "rules":{
    "comma-dangle":"off",
    "no-unused-vars":[
      "warn",
      {
        "vars":"local",
        "args":"none",
      }
    ],
    "no-console":"warn",
    "no-tabs":0,
    "indent": "off",
    "react/jsx-filename-extension":0,
    "no-new":0,
    "no-plusplus": "off",
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures":{
      "jsx":true
    },
    "sourceType":"module",
  }
}